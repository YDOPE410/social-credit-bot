    "dotenv": "^16.0.1",
    "mongoose": "^6.5.2",
    "rimraf": "^3.0.2",
    "telegraf": "^4.8.6",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "concurrently": "^7.3.0",
    "nodemon": "^2.0.19",
